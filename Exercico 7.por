programa {
  funcao inicio() {


  //  7) Crie um algoritmo que leia um número real e mostre na tela o seu dobro e a sua terça parte.//
    
   real apre, n2, n3
    
    n2 = 2.0
    n3 = 3.0

   escreva ("Digite um numero: ")
   leia (apre)

   escreva ("\n", " O dobro de ", apre, " é ", apre*n2 )

   escreva ("\n", " A terça parte de ", apre, " é ", apre/n3 )




  }
}
