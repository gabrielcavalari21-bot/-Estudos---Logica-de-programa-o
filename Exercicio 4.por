programa {
  funcao inicio() {


   // 4) Desenvolva um algoritmo que leia dois números inteiros e mostre o somatório entre eles.//
    
    inteiro n1
    inteiro n2

    escreva ("Digite um numero: ")
    leia (n1)
    escreva ("Digite outro numero: ")
    leia (n2)

    escreva ("A soma entre ", n1, " e ", n2, " é ", " igual a ", n1+n2)


  }
}
